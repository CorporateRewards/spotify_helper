<div class="">
  <div id="tracks-table" class="">
      <table class="table table-hover">
        <th>Song</th><th>Artist</th><th>Your Vote</th><th>Vote</th><th>Actions</th>
        <% @votes.each do |vote| %>
        <tr>
          <td><%= vote.track.name %></td>
          <td><%= vote.track.artist %></td>
          <td><%= vote.vote %></td>            
          <td><%= link_to image_tag("thumbs-up.png", width: "30px"),new_tracks_votes_path(dbid: vote.track.id, vote: 1),class:"btn btn-default vote-btn" %>
              <%= link_to image_tag("thumbs-down.png", width: "30px"),new_tracks_votes_path(dbid: vote.track.id, vote: 0),class:"btn btn-default vote-btn" %></td>
          <td>
              <%= link_to "Play",play_individual_track_path(track: vote.track.uri),class:"btn btn-default", remote: true %>
              <%= link_to "Details",track_details_path(track: vote.track.track_id),class:"btn btn-default" %>
          </td>
        </tr>
        <% end %>
      </table>
    </div>
</div>
