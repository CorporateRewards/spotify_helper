<h1>Results</h1>

	<table class="table">
	  <th>Track name</th><th>Artist</th><th>Actions</th>
	    <% @tracks.each do |track| %>
		  <tr>
		    <td><%= track.name %></td>
			<td><%= track.artists.map(&:name).join(', ') %></td>
			<td><a class="btn btn-default" role="button">Add</a></td>
		  </tr>
		<% end %>
	</table>